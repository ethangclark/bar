{
  "name": "spreader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docker": "docker-compose up -d",
    "dd": "docker-compose down",
    "build": "next build",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/server/db/migrate.ts",
    "dev": "yarn docker && next dev -p 4000",
    "lint": "npx tsc --noEmit && next lint",
    "test": "vitest",
    "lt": "yarn lint && yarn test --run",
    "test:prompts": "TEST_TIMOUT=99999999 TEST_TYPE=prompt_test tsx promptTest.ts",
    "test:coverage": "vitest --run --coverage.reporter=text",
    "start": "yarn db:migrate && next start"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.2",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@anthropic-ai/sdk": "^0.26.1",
