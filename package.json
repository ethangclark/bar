    "@simbathesailor/use-what-changed": "^2.0.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.25.0",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "antd": "^5.17.3",
    "canvas-confetti": "^1.9.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.2",
    "escape-html": "^1.0.3",
    "geist": "^1.3.0",
    "mutative": "^1.0.8",
    "next": "^14.2.1",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.13",
    "openai": "^4.53.0",
    "postgres": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tsx": "^4.11.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/escape-html": "^1.0.4",
    "@types/eslint": "^8.56.2",
